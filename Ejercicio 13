//Franco Vazquez Ian Javier

#include <stdio.h>

void main()
{
    int a, b, c = 0;

    printf("Cantidad de datos: ");
    scanf("%d", &a);

    int v[a];

    for (b = 0; b < a; b++) 
    {
        printf("Dato %d: ", b + 1);
        scanf("%d", &v[b]);
    }

    for (b = 0; b < a; b++) 
    {
        c += v[b];
    }

    printf("Suma total: %d\n", c);
}
