//Franco Vazquez Ian

#include <stdio.h>

void main() 
{
    int a, b, sum, res, mult;
    
    printf("Cual es el primer número?:");
    scanf("%d", &a);
    printf("Cual es el segundo número?:");
    scanf("%d", &b);
    
    sum = a + b;
    res = a - b;
    mult = a * b;
    
    printf("Suma: %d\n", sum);
    printf("Resta: %d\n", res);
    printf("Multiplicacion: %d\n", mult);
}
