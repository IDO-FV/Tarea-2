#include <stdio.h>

void main() {
    int a, b;

    printf("Ingrese el primer número: ");
    scanf("%d", &a);

    printf("Ingrese el segundo número: ");
    scanf("%d", &b);

    if (b % a == 0) {
        printf("%d es divisor de %d\n", a, b);
    }
   
    else if (a % b == 0) {
        printf("%d es divisor de %d\n", b, a);
    }
    else {
        printf("No son numeros divisibles entre si\n");
    }

}
