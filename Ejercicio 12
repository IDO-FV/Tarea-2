#include <stdio.h>

void main() 
{
    int a, b = 1;
    unsigned long long c = 1;

    printf("Ingrese N numeros: ");
    scanf("%d", &a);

    if (a < 1) {
        printf("a debe ser â‰¥ 1.\n");
        return;
    }

    while (b <= a) {
        c *= b;
        b++;
    }

    printf("Producto: %llu\n", c);
}
