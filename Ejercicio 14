#include <stdio.h>

void main()
{
    int a, b, x, c = 0;

    printf("Cantidad de datos: ");
    scanf("%d", &a);

    int v[a];

    for (b = 0; b < a; b++) {
        printf("Dato %d: ", b + 1);
        scanf("%d", &v[b]);
    }

    printf("NÃºmero a buscar: ");
    scanf("%d", &x);

    for (b = 0; b < a; b++) {
        if (v[b] == x) c++;
    }

    printf("El numero %d aparece %d veces.\n", x, c);
}
